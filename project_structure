│       ├── App.jsx
│       ├── main.jsx
│   ├── tailwind.config.js
│   ├── index.html
│   ├── package.json
├── scripts/                    # DevOps, deployment, automation
│   ├── init_db.sql
│   ├── init_db.py
│   ├── deploy_contracts.js     #Hardhat
├── tests/                      # Unit & integration tests
