janus-protocol/
├── contracts/                  # Solidity smart contracts (JNS/JNA/JNO, Vaults, Factory)
│   └── hardhat.config.js
│   └── JNS.sol
│   └── JNA.sol
│   └── JNO.sol
│   └── VaultManager.sol
│   └── StablecoinFactory.sol
├── ai-engine/                  # AI risk modeling
│   ├── app.py                    # FastAPI app entrypoint
│   ├── db.py                     # MySQL database connection via SQLAlchemy
│   ├── models.py                 # ORM models for vaults, tokens, prices
│   ├── services/
│      └── stabilizer.py         # AI logic to evaluate and adjust collateral ratios
│      └──  fx_manager.py       # FX arbitrage logic (FastAPI)
│      └──  yield_optimizer.py
│   ├── routes/
│      └── stabilization.py      # API endpoints for the stabilizer
│      └── fx.py      # API endpoints for the stabilizer
│      └── yield.py      # API endpoints for the stabilizer
│   ├── schemas.py                # Pydantic request/response models
│   ├── config.py                 # Env + MySQL settings
│   ├── requirements.txt          # Python dependencies
├── dashboard/                  # React-based UI
│   ├── public/
│   ├── src/
│       ├── components/
│       │   ├── VaultHealthCard.jsx
